	<table_name>twc_manage_offered</table_name>
	<parent_id>1</parent_id>
	<manage_page_field_list>id,title,description,service_cagegory,published, date_time, status_deleted, sort_order</manage_page_field_list>
	<sort_order_displayed>sort_order</sort_order_displayed>
	<exclude_sort_order>Yes</exclude_sort_order>
	<exclude_publish>Yes</exclude_publish>  
	<img_displayed>img_path</img_displayed>
    <small_img_folder_path>Offered/</small_img_folder_path>

  <forms_twc>
    <fields
		fieldset="Start"
		caption="Messages Details"
		name="fieldset" aligns="left" />
		
	<fields
		display_this_on_form="Y"
		event_on_form="Y"
		name="title"
		type="text"
		datatype="text"
		label="Title"
		validation="Y"
		class="span12"
		validation_description="This is my new text for the Title of Page, You can find it while you edit this page conent." />
	
	
	<fields
		display_this_on_form="Y"
		event_on_form="Y"
		name="description"
		type="text"
		datatype="text"
		label="Description"
		validation="Y"
		class="span12"
		validation_description="This is my new text for the Title of Page, You can find it while you edit this page conent." />
		
	<fields
		display_this_on_form="Y"
		name="service_cagegory"
		type="select"
		datatype="text"
		label="Category"
		validation="Y"
		class="span12"
		dropdownheight=""
		parent_field="N"
		validation_description=""
		multiple_selection="N"
		import_from_table_name="twc_service_category" field1="id" field2="title" field3="">
      <options value="" selected="true">Select</options>
    </fields>	
	
	<fields		

		display_this_on_form="Y"		

		name="pet_shop"		

		type="checkbox"		

		datatype="text"		

		label="Pet Shop"		

		validation="N"		

		class="span12"		

		dropdownheight="250px"		

		parent_field="N"		

		validation_description="Y" 		

		multiple_selection="N" 		

		import_from_table_name="twc_pet_foodtype" field1="id" field2="title" field3="">

	</fields>
	
	
	
	
	
	
	
