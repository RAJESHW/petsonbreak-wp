	<fields
		display_this_on_form="Y"
		name="img_path"
		type="file"
		datatype="text"
		label="Logo"
		validation="N"
		class=""
		sizes="19"
		styles=""
		validation_description="Logo">		
		<options paths="Offered/" crop="No" height="223px" width="" priority_defined_while_cropping = "height"></options>    
	</fields>
    

		
	<fields
		fieldset="End"
		caption="Page Details"
		name="fieldset" />	
	<fields
		fieldset="Start"
		caption="Positioning and Other Details"
		name="fieldset" aligns="right" />	
    <fields
		display_this_on_form="Y"
		name="published"
		type="radio"
		datatype="text"
		label="Published"
		validation="Y"
		class=""
		validation_description="Check on if you want to Publish this Page">
      <options value="Yes" checked="true">Yes</options>
      <options value="No">No</options>
    </fields>
	
    <fields
		display_this_on_form="N"
		name="sort_order"
		type="hidden"
		datatype="int"
